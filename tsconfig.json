{
    "compilerOptions": {
        "outDir": "dist",
        "module": "commonjs",
        "target": "esnext",
        "noImplicitAny": false,
        "sourceMap": false,
        "strictNullChecks": true,
        "allowUnreachableCode": false,
        "allowUnusedLabels": false,
        "forceConsistentCasingInFileNames": true,
        "traceResolution": false,
        "strictFunctionTypes": true,
        "stripInternal": true
    },
    "exclude": [
        "node_modules",
        "dist"
    ]
}